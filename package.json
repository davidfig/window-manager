{
  "name": "simple-window-manager",
  "version": "2.1.0",
  "description": "A vanilla javascript Window Manager with optional application-level menu and keyboard accelerators",
  "main": "public/simple-window-manager.min.js",
  "module": "src/index.js",
  "scripts": {
    "start": "rollup --watch -c docs/rollup.debug.js",
    "docs": "jsdoc -c .jsdoc.json",
    "build-images": "node images/import",
    "build-demo": "rollup -c docs/rollup.config.js",
    "build": "rollup -c rollup.config.js",
    "prepublishOnly": "yarn build-demo && yarn docs && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidfig/window-manager.git"
  },
  "bugs": {
    "url": "https://github.com/davidfig/window-manager/issues"
  },
  "homepage": "https://github.com/davidfig/window-manager#readme",
  "author": "David Figatner (david@yopeyopey.com)",
  "license": "MIT",
  "dependencies": {
    "clicked": "^3.2.3",
    "eventemitter3": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "fs-extra": "^9.0.0",
    "jsdoc": "^3.6.5",
    "rollup": "^2.10.2",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "yy-fps": "^0.6.0",
    "yy-jsdoc-template": "^1.3.0"
  }
}
